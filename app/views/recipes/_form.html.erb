
<%= form_for @recipe do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>

  <%= f.fields_for :ingredients do |f| %>
    <%= f.label :quantity %>
    <%= f.text_field :quantity %><br>

    <%= f.label :name %>
    <%= f.text_field :name  %><br>

  <% end %>
  <%= f.submit %>
<% end %>
